digraph {
	graph [bgcolor="#333333" fontcolor=white fontsize=16 label="Control Flow Graph"]
	node [color=white fillcolor="#006699" fontcolor=white fontname=Helvetica style=filled]
	edge [arrowhead=open color=white fontcolor=white fontname=Courier fontsize=12 style=dashed]
	""
	"int a,b"
	"int c = 10"
	"a = c - 1"
	"c = a + c"
	"" -> "int a,b"
	"int a,b" -> "int c = 10"
	"int c = 10" -> "a = c - 1"
	"a = c - 1" -> "c = a + c"
}
